---
import Card from "../components/Card.astro";
interface Props {
    row?: any;
}
const { row } = Astro.props;
---

<div class="row slide-in-from-bottom-light">
    {
        row.map((post) => (
            <div class="column to-slide-bottom-light animate">
                <Card article={post} />
            </div>
        ))
    }
</div>
